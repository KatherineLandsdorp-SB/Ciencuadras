<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name = "regression">
    <listeners>
        <listener class-name="com.segurosbolivar.automation.commons.TestListener"/>
    </listeners>
<test name="Seguros Bolivar Test" >
    <classes>
        <class name="com.segurosbolivar.automation.tests.login.RunnerLogin">
            <methods>
                <include name="loginWithoutRegistration"/>
                <include name="loginWithEmptyFields"/>
                <include name="loginWithInvalidMailFormat"/>
                <include name="loginSuccessfully"/>
            </methods>
        </class>
    </classes>
</test>
</suite>